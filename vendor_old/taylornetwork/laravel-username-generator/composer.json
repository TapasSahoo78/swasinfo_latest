{
    "name": "taylornetwork/laravel-username-generator",
    "require": {
        "illuminate/support": ">=5.5",
        "php": "^7.4|^8.0|^8.1",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "orchestra/testbench": "*",
        "psy/psysh": "*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Sam Taylor",
            "email": "sam@taylornetwork.ca"
        }
    ],
    "autoload": {
        "psr-4": {
            "TaylorNetwork\\UsernameGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TaylorNetwork\\UsernameGenerator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TaylorNetwork\\UsernameGenerator\\ServiceProvider"
            ],
            "aliases": {
                "UsernameGenerator": "TaylorNetwork\\UsernameGenerator\\Facades\\UsernameGenerator"
            }
        }
    }
}
